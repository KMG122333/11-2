#include <stdio.h>
#define MUL(x, y) x*y

int main() {
    int a, b;
    double c, d;

    printf("두개의 정수를 입력하세요: ");
    scanf("%d %d", &a, &b);
    printf("%d *%d = %d\n", a, b, MUL(a, b));
    printf("두개의 실수를 입력하세요: ");
    scanf("%lf %lf", &c, &d);
    printf("%lf * %lf = %lf \n", c, d, MUL(c, d));
    return 0;
}
